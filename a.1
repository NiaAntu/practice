//C program to find diameter, circumference and area of circle using function.


#include <stdio.h>
#include <math.h>
#define N 3.14
double getDiameter(double x)
{
    return (2 * x);
}
double getCircumference(double x)
{
    return (2 * N * x);
}
double getArea(double x)
{
    return (N * x * x);
}

int main()
{
    double x, r1, r2, r3;
    scanf("%lf", &x);
    r1 = getDiameter(x);
    r2 = getCircumference(x);
    r3 = getArea(x);

    printf("dia = %.2lf\ncir = %.2lf\narea = %.2lf ", r1, r2, r3);

    return 0;
}
